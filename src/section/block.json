{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tt-custom-blocks/section",
	"version": "0.1.0",
	"title": "Section Block",
	"category": "widgets",
	"description": "Display custom section tag with various options",
	"example": {},
	"attributes": {
		"layoutType": {
			"type": "string",
			"default": "fullwidth"
		},
		"padding": {
			"type": "object",
			"default": {
				"xs": "-",
				"sm": "-",
				"md": "-",
				"lg": "-",
				"xl": "-",
				"2xl": "-"
			}
		}
	},
	"supports": {
		"jsx": true,
		"color": {
			"background": true,
			"text": false
		}
	},
	"textdomain": "tt-theme-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
