{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tt-theme-blocks/section",
	"version": "0.1.0",
	"title": "Section Block",
	"category": "tt-theme-blocks-category",
	"description": "Display custom section tag with various options",
	"example": {},
	"attributes": {
		"sectionLayout": {
			"type": "string",
			"default": "fullwidth"
		},
		"sectionHtmlElement": {
			"type": "string",
			"default": "section"
		},
		"sectionPadding": {
			"type": "object",
			"default": {
				"xs": "-",
				"sm": "-",
				"md": "-",
				"lg": "-",
				"xl": "-",
				"2xl": "-"
			}
		},
		"sectionSpacing": {
			"type": "object",
			"default": {
				"xs": "-",
				"sm": "-",
				"md": "-",
				"lg": "-",
				"xl": "-",
				"2xl": "-"
			}
		},
		"sectionBackground": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "tt-theme-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
