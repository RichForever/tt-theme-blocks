{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tt-custom-blocks/section",
	"version": "0.1.0",
	"title": "Section Block",
	"category": "widgets",
	"description": "Display custom section tag with various options",
	"example": {},
	"attributes": {
		"layoutType": {
			"type": "string",
			"default": "fullwidth"
		},
		"blockVerticalPadding": {
			"type": "object",
			"default": {
				"paddingXs": "0",
				"paddingSm": "0",
				"paddingMd": "0",
				"paddingLg": "0",
				"paddingXl": "0",
				"padding2xl": "0"
			}
		}
	},
	"supports": {
		"jsx": true
	},
	"textdomain": "section-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
