{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tt-theme-blocks/icon-picker",
	"version": "0.1.0",
	"title": "Icon Picker Block",
	"category": "widgets",
	"description": "Display custom icon with various options",
	"example": {},
	"attributes": {
		"icon": {
			"type": "string",
			"default": ""
		},
		"iconSize": {
			"type": "object",
			"default": {
				"xs": "-",
				"sm": "-",
				"md": "-",
				"lg": "-",
				"xl": "-",
				"2xl": "-"
			}
		},
		"iconPadding": {
			"type": "object",
			"default": {
				"xs": "-",
				"sm": "-",
				"md": "-",
				"lg": "-",
				"xl": "-",
				"2xl": "-"
			}
		},
		"iconOverriddeFill": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"color": {
			"background": true,
			"text": true
		}
	},
	"textdomain": "tt-theme-blocks",
	"editorScript": ["file:./index.js"],
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
